#: libgksu.c:1356:1
#: libgksu.c:1385:1
#: libgksu.c:1415:1
#: libgksu.c:1644:1
msgid "@context: the #GksuContext from which to grab the information"
msgstr "@context: 从给予的信息中获取 #GksuContext"

# #used by: gksu_context_get_user:
#: libgksu.c:1321:2
msgid "Gets the user the command will be run as, as set by gksu_context_set_user."
msgstr "取得运行命令的用户，设置则用gksu_context_set_user。"

# #used by: gksu_context_get_user:
#: libgksu.c:1324:1
msgid "Returns: a string with the user or NULL if not set."
msgstr "Returns:包含用户名的字符串，如果没有设置则返回NULL"

# used by: gksu_context_set_command:
#: libgksu.c:1335:1
msgid "@command: the command that shall be ran"
msgstr "@command: 将运行的命令。"

# #used by: gksu_context_set_command:
#: libgksu.c:1337:1
msgid "Sets up what command will run with the target user."
msgstr "替目标用户设置将运行的命令"

# #used by: gksu_context_get_command:
#: libgksu.c:1358:2
msgid "Gets the command that will be run, as set by gksu_context_set_command."
msgstr "获取将运行的命令， 设置则为 gksu_context_set_command "

# #used by: gksu_context_get_command:
#: libgksu.c:1361:1
msgid "Returns: a string with the command or NULL if not set."
msgstr "Returns:包含命令的字符串，如果没有设置则返回NULL"

# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# used by: gksu_context_set_login_shell:
# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# used by: gksu_context_set_keep_env:
# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# used by: gksu_context_set_debug:
