msgid "@file: file for the folder to add"
msgstr ""

# #used by: _gtk_file_chooser_add_shortcut_folder:
#: gtkfilechooser.c:2018:2
msgid ""
"Adds a folder to be displayed with the shortcut folders in a file chooser. "
"Internal function, see gtk_file_chooser_add_shortcut_folder()."
msgstr ""

# #used by: _gtk_file_chooser_add_shortcut_folder:
#: gtkfilechooser.c:2021:2
msgid ""
"Return value: %TRUE if the folder could be added successfully, %FALSE "
"otherwise."
msgstr ""

# used by: _gtk_file_chooser_remove_shortcut_folder:
#: gtkfilechooser.c:2040:1
msgid "@file: file for the folder to remove"
msgstr ""

# #used by: _gtk_file_chooser_remove_shortcut_folder:
#: gtkfilechooser.c:2043:2
