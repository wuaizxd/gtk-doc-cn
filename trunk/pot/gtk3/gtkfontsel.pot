#: gtkfontsel.c:1539:3 gtkfontsel.c:1822:3
msgid ""
"Return value: the text displayed in the preview area. This string is owned "
"by the widget and should not be modified or freed"
msgstr ""

# #-#-#-#-#  gtkfontsel.pot (PACKAGE VERSION)  #-#-#-#-#
# used by: gtk_font_selection_set_preview_text:
# #-#-#-#-#  gtkfontsel.pot (PACKAGE VERSION)  #-#-#-#-#
# used by: gtk_font_selection_dialog_set_preview_text:
#: gtkfontsel.c:1559:1 gtkfontsel.c:1841:1
msgid "@text: the text to display in the preview area"
msgstr ""

# #used by: gtk_font_selection_set_preview_text:
#: gtkfontsel.c:1561:2
msgid ""
"Sets the text displayed in the preview area. The @text is used to show how "
"the selected font looks."
msgstr ""

