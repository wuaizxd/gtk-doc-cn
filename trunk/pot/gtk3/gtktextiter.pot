#: gtktextiter.c:2191:1 gtktextiter.c:2259:1 gtktextiter.c:2330:1
#: gtktextiter.c:2443:1 gtktextiter.c:2464:1 gtktextiter.c:2613:1
#: gtktextiter.c:2664:1 gtktextiter.c:2781:1 gtktextiter.c:2821:1
msgid "Return value: whether @iter moved and is dereferenceable"
msgstr ""

#. used by: gtk_text_iter_backward_char:
#: gtktextiter.c:2215:4
msgid ""
"Moves backward by one character offset. Returns %TRUE if movement was "
"possible; if @iter was the first in the buffer (character offset 0), "
"gtk_text_iter_backward_char () returns %FALSE for convenience when writing "
"loops."
msgstr ""

#. used by: gtk_text_iter_backward_char:
#: gtktextiter.c:2220:1
msgid "Return value: whether movement was possible"
msgstr ""

#. used by: gtk_text_iter_forward_chars:
#: gtktextiter.c:2250:1
msgid "@count: number of characters to move, may be negative"
msgstr ""

#. used by: gtk_text_iter_forward_chars:
#: gtktextiter.c:2252:6
msgid ""
"Moves @count characters if possible (if @count would move past the start or "
"end of the buffer, moves to the start or end of the buffer). The return "
"value indicates whether the new position of @iter is different from its "
"original position, and dereferenceable (the last iterator in the buffer is "
"not dereferenceable). If @count is 0, the function does nothing and returns "
"%FALSE."
msgstr ""

#. used by: gtk_text_iter_backward_chars:
#: gtktextiter.c:2321:1
msgid "@count: number of characters to move"
msgstr ""

#. used by: gtk_text_iter_backward_chars:
#: gtktextiter.c:2323:6
msgid ""
"Moves @count characters backward, if possible (if @count would move past the "
"start or end of the buffer, moves to the start or end of the buffer).  The "
"return value indicates whether the iterator moved onto a dereferenceable "
"position; if the iterator didn't move, or moved onto the end iterator, then "
"%FALSE is returned. If @count is 0, the function does nothing and returns "
"%FALSE."
msgstr ""

#. used by: gtk_text_iter_forward_text_chars:
#: gtktextiter.c:2436:1 gtktextiter.c:2457:1
msgid "@count: number of chars to move"
msgstr ""

#. used by: gtk_text_iter_forward_text_chars:
#: gtktextiter.c:2438:4
msgid ""
"Moves forward by @count text characters (pixbufs, widgets, etc. do not count "
"as characters for this). Equivalent to moving through the results of "
"gtk_text_iter_get_text (), rather than gtk_text_iter_get_slice ()."
msgstr ""

#. used by: gtk_text_iter_forward_text_chars:
#: gtktextiter.c:2459:4
msgid ""
"Moves backward by @count text characters (pixbufs, widgets, etc. do not "
"count as characters for this). Equivalent to moving through the results of "
"gtk_text_iter_get_text (), rather than gtk_text_iter_get_slice ()."
msgstr ""

#. used by: gtk_text_iter_forward_line:
#: gtktextiter.c:2479:4
msgid ""
"Moves @iter to the start of the next line. If the iter is already on the "
"last line of the buffer, moves the iter to the end of the current line. If "
"after the operation, the iter is at the end of the buffer and not "
"dereferencable, returns %FALSE. Otherwise, returns %TRUE."
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_line:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_visible_line:
#: gtktextiter.c:2484:1 gtktextiter.c:2700:1
msgid "Return value: whether @iter can be dereferenced"
msgstr ""

#. used by: gtk_text_iter_backward_line:
#: gtktextiter.c:2528:7
msgid ""
"Moves @iter to the start of the previous line. Returns %TRUE if @iter could "
"be moved; i.e. if @iter was at character offset 0, this function returns "
"%FALSE. Therefore if @iter was already on line 0, but not at the start of "
"the line, @iter is snapped to the start of the line and the function returns "
"%TRUE. (Note that this implies that in a loop calling this function, the "
"line number may not change on every iteration, if your first iteration is on "
"line 0.)"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_line:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_visible_line:
#: gtktextiter.c:2536:1 gtktextiter.c:2740:1
msgid "Return value: whether @iter moved"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_lines:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_visible_lines:
#: gtktextiter.c:2603:1 gtktextiter.c:2771:1
msgid "@count: number of lines to move forward"
msgstr ""

#. used by: gtk_text_iter_forward_lines:
#: gtktextiter.c:2605:7
msgid ""
"Moves @count lines forward, if possible (if @count would move past the start "
"or end of the buffer, moves to the start or end of the buffer).  The return "
"value indicates whether the iterator moved onto a dereferenceable position; "
"if the iterator didn't move, or moved onto the end iterator, then %FALSE is "
"returned. If @count is 0, the function does nothing and returns %FALSE. If "
"@count is negative, moves backward by 0 - @count lines."
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_lines:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_visible_lines:
#: gtktextiter.c:2654:1 gtktextiter.c:2811:1
msgid "@count: number of lines to move backward"
msgstr ""

#. used by: gtk_text_iter_backward_lines:
#: gtktextiter.c:2656:7
msgid ""
"Moves @count lines backward, if possible (if @count would move past the "
"start or end of the buffer, moves to the start or end of the buffer).  The "
"return value indicates whether the iterator moved onto a dereferenceable "
"position; if the iterator didn't move, or moved onto the end iterator, then "
"%FALSE is returned. If @count is 0, the function does nothing and returns "
"%FALSE. If @count is negative, moves forward by 0 - @count lines."
msgstr ""

#. used by: gtk_text_iter_forward_visible_line:
#: gtktextiter.c:2695:4
msgid ""
"Moves @iter to the start of the next visible line. Returns %TRUE if there "
"was a next line to move to, and %FALSE if @iter was simply moved to the end "
"of the buffer and is now not dereferenceable, or if @iter was already at the "
"end of the buffer."
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_visible_line:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_visible_line:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_visible_lines:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_visible_lines:
#: gtktextiter.c:2702:1 gtktextiter.c:2742:1 gtktextiter.c:2783:1
#: gtktextiter.c:2823:1
msgid "Since: 2.8"
msgstr ""

#. used by: gtk_text_iter_backward_visible_line:
#: gtktextiter.c:2732:7
msgid ""
"Moves @iter to the start of the previous visible line. Returns %TRUE if "
"@iter could be moved; i.e. if @iter was at character offset 0, this function "
"returns %FALSE. Therefore if @iter was already on line 0, but not at the "
"start of the line, @iter is snapped to the start of the line and the "
"function returns %TRUE. (Note that this implies that in a loop calling this "
"function, the line number may not change on every iteration, if your first "
"iteration is on line 0.)"
msgstr ""

#. used by: gtk_text_iter_forward_visible_lines:
#: gtktextiter.c:2773:7
msgid ""
"Moves @count visible lines forward, if possible (if @count would move past "
"the start or end of the buffer, moves to the start or end of the buffer).  "
"The return value indicates whether the iterator moved onto a dereferenceable "
"position; if the iterator didn't move, or moved onto the end iterator, then "
"%FALSE is returned. If @count is 0, the function does nothing and returns "
"%FALSE. If @count is negative, moves backward by 0 - @count lines."
msgstr ""

#. used by: gtk_text_iter_backward_visible_lines:
#: gtktextiter.c:2813:7
msgid ""
"Moves @count visible lines backward, if possible (if @count would move past "
"the start or end of the buffer, moves to the start or end of the buffer).  "
"The return value indicates whether the iterator moved onto a dereferenceable "
"position; if the iterator didn't move, or moved onto the end iterator, then "
"%FALSE is returned. If @count is 0, the function does nothing and returns "
"%FALSE. If @count is negative, moves forward by 0 - @count lines."
msgstr ""

#. used by: gtk_text_iter_forward_word_end:
#: gtktextiter.c:3190:5
msgid ""
"Moves forward to the next word end. (If @iter is currently on a word end, "
"moves forward to the next one after that.) Word breaks are determined by "
"Pango and should be correct for nearly any language (if not, the correct fix "
"would be to the Pango word break algorithms)."
msgstr ""

