msgid "@x: X position to move to"
msgstr ""

#. used by: gtk_layout_move:
#: gtklayout.c:515:1
msgid "@y: Y position to move to"
msgstr ""

#. used by: gtk_layout_move:
#: gtklayout.c:517:1
msgid "Moves a current child of @layout to a new position."
msgstr ""

#. used by: gtk_layout_set_size:
