msgstr ""

# used by: SECTION:timezone
#: gtimezone.c:40:1
msgid "@title: GTimeZone"
msgstr ""

# used by: SECTION:timezone
#: gtimezone.c:41:1
msgid "@short_description: A structure representing a time zone"
msgstr ""

# #used by: g_time_zone_new_utc:
#: gtimezone.c:410:1
msgid "Creates a #GTimeZone corresponding to UTC."
msgstr ""

# #used by: g_time_zone_new_utc:
#: gtimezone.c:412:2
msgid ""
"This is equivalent to calling g_time_zone_new() with a value like |$_1_$|Z|"
"$_1_$|, |$_1_$|UTC|$_1_$|, |$_1_$|+00|$_1_$|, etc."
msgstr ""

# #used by: g_time_zone_new_utc:
#: gtimezone.c:418:1
msgid "Returns: the universal timezone"
msgstr ""

# used by: SECTION:timezone
#: gtimezone.c:42:1
msgid "@see_also: #GDateTime"
msgstr ""

# #used by: g_time_zone_new_local:
#: gtimezone.c:431:1
msgid "Creates a #GTimeZone corresponding to local time."
msgstr ""

# #used by: g_time_zone_new_local:
#: gtimezone.c:433:4
msgid ""
"This is equivalent to calling g_time_zone_new() with the value of the "
"<varname>TZ</varname> environment variable (including the possibility of "
"%NULL).  Changes made to <varname>TZ</varname> after the first call to this "
"function may or may not be noticed by future calls."
msgstr ""

# #used by: SECTION:timezone
#: gtimezone.c:44:2
msgid ""
"#GTimeZone is a structure that represents a time zone, at no particular "
"point in time.  It is refcounted and immutable."
msgstr ""

# #used by: g_time_zone_new_local:
#: gtimezone.c:441:1
msgid "Returns: the local timezone"
msgstr ""

# #used by: SECTION:timezone
#: gtimezone.c:47:1
msgid "#GTimeZone is available since GLib 2.26."
msgstr ""

# #used by: GTimeZone:
#: gtimezone.c:53:2
msgid ""
"#GDateTime is an opaque structure whose members cannot be accessed directly."
msgstr ""

