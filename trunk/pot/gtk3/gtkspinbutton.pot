#: gtkspinbutton.c:438:5
msgid ""
"adjustment = gtk_spin_button_get_adjustment (spin); value = (int)"
"gtk_adjustment_get_value (adjustment); text = g_strdup_printf (\"%02d\", "
"value); gtk_entry_set_text (GTK_ENTRY (spin), text); g_free (text);"
msgstr ""

#. used by: GtkSpinButton::output:
#: gtkspinbutton.c:444:3
msgid "return TRUE; } ]|"
msgstr ""

#. used by: GtkSpinButton::output:
#: gtkspinbutton.c:448:1
msgid "Returns: %TRUE if the value has been displayed"
msgstr ""

