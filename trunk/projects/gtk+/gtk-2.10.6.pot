msgid "Recursively expands all nodes in the @tree_view."
msgstr ""

# #used by: gtk_tree_view_collapse_all:
#: gtktreeview.c:11609:1
msgid "Recursively collapses all visible, expanded nodes in @tree_view."
msgstr ""

# used by: gtk_tree_view_expand_to_path:
#: gtktreeview.c:11647:1
msgid "@path: path to a row."
msgstr ""

# #used by: gtk_tree_view_expand_to_path:
#: gtktreeview.c:11649:2
