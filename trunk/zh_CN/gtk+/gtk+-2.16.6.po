#: gtktreeviewcolumn.c:1453:7
msgid ""
"column = gtk_tree_view_column_new_with_attributes (|$_1_$|Title|$_1_$|, "
"renderer, |$_1_$|text|$_1_$|, TEXT_COLUMN, |$_1_$|foreground|$_1_$|, "
"COLOR_COLUMN, NULL); } ]|"
msgstr ""

