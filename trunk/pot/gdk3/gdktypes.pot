#. used by: GdkEventMask:
#: gdktypes.h:269:1
msgid "@GDK_EXPOSURE_MASK: receive expose events"
msgstr ""

#. used by: GdkEventMask:
#: gdktypes.h:270:1
msgid "@GDK_POINTER_MOTION_MASK: receive all pointer motion events"
msgstr ""

#. used by: GdkEventMask:
#: gdktypes.h:271:1
msgid "@GDK_POINTER_MOTION_HINT_MASK: see the explanation above"
msgstr ""

#. used by: GdkEventMask:
#: gdktypes.h:272:4
