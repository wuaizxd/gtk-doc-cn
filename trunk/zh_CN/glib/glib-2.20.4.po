msgid "@time_: a #GTimeVal"
msgstr ""

# used by: g_time_val_add:
#: gtimer.c:221:1
msgid "@microseconds: number of microseconds to add to @time"
msgstr ""

# #used by: g_time_val_add:
#: gtimer.c:223:2
msgid ""
"Adds the given number of microseconds to @time_. @microseconds can also be "
"negative to decrease the value of @time_."
msgstr ""

# used by: g_time_val_from_iso8601:
#: gtimer.c:290:1
msgid "@iso_date: an ISO 8601 encoded date string"
msgstr ""

# #used by: g_time_val_from_iso8601:
#: gtimer.c:293:2
msgid ""
"Converts a string containing an ISO 8601 encoded date and time to a "
"#GTimeVal and puts it into @time_."
msgstr ""

# #used by: g_time_val_from_iso8601:
#: gtimer.c:296:1
msgid "Return value: %TRUE if the conversion was successful."
msgstr ""

# #used by: g_time_val_to_iso8601:
#: gtimer.c:407:2
msgid ""
"Converts @time_ into an ISO 8601 encoded string, relative to the Coordinated "
"Universal Time (UTC)."
msgstr ""

# #used by: g_time_val_to_iso8601:
#: gtimer.c:410:1
msgid "Return value: a newly allocated string containing an ISO 8601 date"
msgstr ""

# used by: g_tree_new:
#: gtree.c:114:5
msgid ""
"@key_compare_func: the function used to order the nodes in the #GTree. It "
"should return values similar to the standard strcmp() function - 0 if the "
"two arguments are equal, a negative value if the first argument comes before "
"the second, or a positive value if the first argument comes after the second."
msgstr ""

# #used by: g_tree_new:
#: gtree.c:120:1
msgid "Creates a new #GTree."
msgstr ""

# #-#-#-#-#  gtree.pot~ (glib 2.12.7)  #-#-#-#-#
# #used by: g_tree_new:
# #-#-#-#-#  gtree.pot~ (glib 2.12.7)  #-#-#-#-#
