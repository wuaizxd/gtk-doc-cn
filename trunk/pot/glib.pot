msgid "@max_len: max bytes to validate, or -1 to go until NUL"
msgstr ""

# used by: g_utf8_validate:
#: gutf8.c:1755:1
msgid "@end: return location for end of valid data"
msgstr ""

# #used by: g_utf8_validate:
#: gutf8.c:1757:7
