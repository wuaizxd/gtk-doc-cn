#: gmarkup.c:2561:1
msgid "@element_name: the current tag name"
msgstr ""

# used by: g_markup_collect_attributes:
#. used by: g_markup_collect_attributes:
#: gmarkup.c:2562:1
msgid "@attribute_names: the attribute names"
msgstr ""

# used by: g_markup_collect_attributes:
