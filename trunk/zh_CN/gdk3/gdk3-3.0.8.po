msgid "@window: a #GdkWindow to simulate a button event for"
msgstr ""

# used by: gdk_test_simulate_button:
#: gdkwindow.c:10719:1
msgid "@x:      x coordinate within @window for the button event"
msgstr ""

# used by: gdk_test_simulate_button:
#: gdkwindow.c:10720:1
msgid "@y:      y coordinate within @window for the button event"
msgstr ""

# used by: gdk_test_simulate_button:
