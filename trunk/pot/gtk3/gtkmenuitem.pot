msgid "Since: 2.14"
msgstr ""

#. used by: gtk_menu_item_set_label:
#: gtkmenuitem.c:2483:1
msgid "@label: the text you want to set"
msgstr ""

#. #-#-#-#-#  gtkmenuitem.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_menu_item_set_label:
#. #-#-#-#-#  gtkmenuitem.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_menu_item_get_label:
#: gtkmenuitem.c:2485:1 gtkmenuitem.c:2502:1
msgid "Sets @text on the @menu_item label"
msgstr ""

#. #-#-#-#-#  gtkmenuitem.pot (PACKAGE VERSION)  #-#-#-#-#
