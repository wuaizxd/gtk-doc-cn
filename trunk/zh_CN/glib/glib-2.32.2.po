#: gutf8.c:452:1
msgid "@c: a Unicode character code"
msgstr ""

# used by: g_unichar_to_utf8:
#. used by: g_unichar_to_utf8:
#: gutf8.c:453:3
msgid ""
"@outbuf: output buffer, must have at least 6 bytes of space. If %NULL, the "
"length will be computed and returned and nothing will be written to @outbuf."
msgstr ""

#. used by: g_unichar_to_utf8:
#: gutf8.c:457:1
msgid "Converts a single character to UTF-8."
msgstr ""

#. used by: g_unichar_to_utf8:
#: gutf8.c:459:1
msgid "Return value: number of bytes written"
msgstr ""

