#. used by: gtk_entry_set_icon_tooltip_markup:
#: gtkentry.c:8370:1 gtkentry.c:8408:1 gtkentry.c:8454:1 gtkentry.c:8487:1
msgid "@icon_pos: the icon position"
msgstr ""

#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_get_icon_tooltip_text:
#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_get_icon_tooltip_markup:
#: gtkentry.c:8372:2 gtkentry.c:8456:2
msgid ""
"Gets the contents of the tooltip on the icon at the specified position in "
"@entry."
msgstr ""

#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_get_icon_tooltip_text:
#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_get_icon_tooltip_markup:
#: gtkentry.c:8375:2 gtkentry.c:8459:2
msgid ""
"Returns: the tooltip text, or %NULL. Free the returned string with g_free() "
"when done."
msgstr ""

#. used by: GtkEntry:xalign:
#: gtkentry.c:838:2
msgid ""
"The horizontal alignment, from 0 (left) to 1 (right). Reversed for RTL "
"layouts."
msgstr ""

#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_set_icon_tooltip_text:
#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_set_icon_tooltip_markup:
#: gtkentry.c:8409:1 gtkentry.c:8488:1
msgid ""
"@tooltip: (allow-none): the contents of the tooltip for the icon, or %NULL"
msgstr ""

#. used by: gtk_entry_set_icon_tooltip_text:
#: gtkentry.c:8411:2
msgid ""
"Sets @tooltip as the contents of the tooltip for the icon at the specified "
"position."
msgstr ""

#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_set_icon_tooltip_text:
#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_entry_set_icon_tooltip_markup:
#: gtkentry.c:8414:1 gtkentry.c:8494:1
msgid "Use %NULL for @tooltip to remove an existing tooltip."
msgstr ""

#. used by: gtk_entry_set_icon_tooltip_text:
#: gtkentry.c:8416:2
msgid ""
"See also gtk_widget_set_tooltip_text() and gtk_entry_set_icon_tooltip_markup"
"()."
msgstr ""

#. used by: gtk_entry_set_icon_tooltip_markup:
#: gtkentry.c:8490:3
msgid ""
"Sets @tooltip as the contents of the tooltip for the icon at the specified "
"position. @tooltip is assumed to be marked up with the <link linkend="
"\"PangoMarkupFormat\">Pango text markup language</link>."
msgstr ""

#. used by: gtk_entry_set_icon_tooltip_markup:
#: gtkentry.c:8496:2
msgid ""
"See also gtk_widget_set_tooltip_markup() and gtk_enty_set_icon_tooltip_text"
"()."
msgstr ""

#. used by: GtkEntry:truncate-multiline:
#: gtkentry.c:856:1
msgid "When %TRUE, pasted multi-line text is truncated to the first line."
msgstr ""

#. used by: GtkEntry:shadow-type:
#: gtkentry.c:871:2
msgid ""
"Which kind of shadow to draw around the entry when #GtkEntry:has-frame is "
"set to %TRUE."
msgstr ""

#. #-#-#-#-#  gtkentry.pot (PACKAGE VERSION)  #-#-#-#-#
