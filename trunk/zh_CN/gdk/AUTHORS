#!/bin/sh
#此文件的格式如下
#作者 邮箱 sgml文件数组 po文件数组
#sgml_files和po_files数组内容以","分隔，中间不要使用空格，否则项目脚本将出错。
#作者名为unknown表示无人认领，未开始翻译
#作者名为finished表示已内容已经翻译完成。
yetist yetist@gmail.com sgml_files=(gdk-docs.sgml,tmpl/threads.sgml) po_files=(gdk.po,gdkvisual.po)
unknown unknown@nomail.com sgml_files=(gdk-docs.sgml,multihead.sgml,tmpl/cairo_interaction.sgml,tmpl/colors.sgml,tmpl/cursors.sgml,tmpl/dnd.sgml,tmpl/drawing.sgml,tmpl/event_structs.sgml,tmpl/events.sgml,tmpl/fonts.sgml,tmpl/gcs.sgml,tmpl/gdk-unused.sgml,tmpl/gdkapplaunchcontext.sgml,tmpl/gdkdisplay.sgml,tmpl/gdkdisplaymanager.sgml,tmpl/gdkscreen.sgml,tmpl/gdktesting.sgml,tmpl/general.sgml,tmpl/images.sgml,tmpl/input.sgml,tmpl/input_devices.sgml,tmpl/keys.sgml,tmpl/pango_interaction.sgml,tmpl/pixbufs.sgml,tmpl/pixmaps.sgml,tmpl/properties.sgml,tmpl/regions.sgml,tmpl/rgb.sgml,tmpl/selections.sgml,tmpl/threads.sgml,tmpl/visuals.sgml,tmpl/windows.sgml,tmpl/x_interaction.sgml) po_files=(gdkapplaunchcontext.po,gdkcairo.po,gdkcolor.po,gdkcursor.po,gdkdisplay.po,gdkdisplaymanager.po,gdkdnd.po,gdkdraw.po,gdkevents.po,gdkfont.po,gdkgc.po,gdkimage.po,gdkkeys.po,gdkkeyuni.po,gdkpango.po,gdkpixbuf-drawable.po,gdkpixbuf-render.po,gdkpolyreg-generic.po,gdkrectangle.po,gdkregion-generic.po,gdkrgb.po,gdkscreen.po,gdkselection.po,gdkvisual.po,gdkwindow.po)
