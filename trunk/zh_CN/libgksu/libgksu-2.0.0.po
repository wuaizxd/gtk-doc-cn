#: libgksu.c:803:1
#: libgksu.c:1683:1
#: libgksu.c:2147:1
#: libgksu.c:2617:1
#: libgksu.c:2697:1
msgid "@context: a #GksuContext"
msgstr "@context: a #GksuContext"

# used by: su_ask_password:
#: libgksu.c:804:1
msgid "@prompt: the prompt that should be used instead of |$_1_$|Password: |$_1_$|"
msgstr "@prompt: 代替 |$_1_$|密码: |$_1_$|的提示信息。"

# used by: su_ask_password:
#: libgksu.c:805:1
msgid "@data: data that is passed by gksu_*_full"
msgstr "@data: 通过gksu_*_full传递来的参数"

# used by: su_ask_password:
#: libgksu.c:806:2
msgid "@error: a pointer to pointer #GError that will be filled with data if an error happens."
msgstr "@error: 如果有错误发生，指针的指针 #GError 将被填充。"

# #used by: su_ask_password:
#: libgksu.c:809:2
msgid "This is a convenience function to create a #GksuuiDialog and request the password."
msgstr "这是一个方便的函数，可以直接调用产生 #GksuuiDialog，并要求输入密码。"

# #used by: su_ask_password:
#: libgksu.c:812:2
msgid "Returns: a newly allocated gchar containing the password or NULL if an error happens or the user cancels the action"
msgstr "Returns:  gchar类型，包含用户输入的密码，发生错误或者用户取消时返回NULL。"

# #used by: prepare_xauth:
#: libgksu.c:1054:3
msgid "Sets up the variables with values for the $DISPLAY environment variable and xauth-related stuff. Also creates a temporary directory to hold a .Xauthority"
msgstr "设置环境变量$DISPLAY的值，并填充认证关联。同样会创建一个临时目录去控制 .Xauthority"

# #used by: prepare_xauth:
#: libgksu.c:1058:1
msgid "Returns: TRUE if it suceeds, FALSE if it fails."
msgstr "Returns:如果成功返回TURE,失败返回FALSE"

# #used by: gksu_context_new
#: libgksu.c:1256:3
msgid "This function should be used when creating a new #GksuContext to pass to gksu_su_full or gksu_sudo_full. The #GksuContext must be freed with gksu_context_free."
msgstr "这个函数将创建一个新的 #GksuContext 对象，用以传递给 gksu_su_full  或者 gksu_sudo_full 。#GksuContext 对象必须使用 gksu_context_free 释放掉。"

# #used by: gksu_context_new
#: libgksu.c:1260:1
msgid "Returns: a newly allocated #GksuContext"
msgstr "Returns:返回一个 #GksuContext 对象"

