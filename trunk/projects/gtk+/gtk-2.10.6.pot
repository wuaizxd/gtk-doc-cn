msgid "Gets the value set by gtk_entry_set_width_chars()."
msgstr ""

# #used by: gtk_entry_get_width_chars:
#: gtkentry.c:4434:1
msgid ""
"Return value: number of chars to request space for, or negative if unset"
msgstr ""

# used by: gtk_entry_set_has_frame:
#: gtkentry.c:4447:1
msgid "@setting: new value"
msgstr ""

# #used by: gtk_entry_set_has_frame:
#: gtkentry.c:4449:1
