#: gtktextlayout.c:2867:3
msgid ""
"Find the range of X coordinates for the paragraph containing the given iter. "
"Private for 2.0 due to API freeze, could be made public for 2.2."
msgstr ""

# used by: gtk_text_layout_set_buffer:
#: gtktextlayout.c:303:1
msgid "@buffer: (allow-none):"
msgstr ""

# used by: gtk_text_layout_clamp_iter_to_vrange:
#: gtktextlayout.c:3082:1
msgid "@top:    the top of the range"
msgstr ""

# used by: gtk_text_layout_clamp_iter_to_vrange:
#: gtktextlayout.c:3083:1
msgid "@bottom: the bottom the range"
msgstr ""

# #used by: gtk_text_layout_clamp_iter_to_vrange:
#: gtktextlayout.c:3085:3
msgid ""
"If the iterator is not fully in the range @top <= y < @bottom, then, if "
"possible, move it the minimum distance so that the iterator in this range."
msgstr ""

# #used by: gtk_text_layout_clamp_iter_to_vrange:
#: gtktextlayout.c:3089:1
msgid "Returns: %TRUE if the iterator was moved, otherwise %FALSE."
msgstr ""

# #used by: gtk_text_layout_move_iter_to_previous_line:
#: gtktextlayout.c:3129:2
msgid ""
"Move the iterator to the beginning of the previous line. The lines of a "
"wrapped paragraph are treated as distinct for this operation."
msgstr ""

# #used by: gtk_text_layout_move_iter_to_next_line:
#: gtktextlayout.c:3248:3
msgid ""
"Move the iterator to the beginning of the next line. The lines of a wrapped "
"paragraph are treated as distinct for this operation."
msgstr ""

# used by: gtk_text_layout_move_iter_to_line_end:
#: gtktextlayout.c:3323:2
msgid ""
"@direction: if negative, move to beginning of line, otherwise move to end of "
"line."
msgstr ""

# #used by: gtk_text_layout_move_iter_to_line_end:
#: gtktextlayout.c:3326:1
msgid "Move to the beginning or end of a display line."
msgstr ""

# used by: gtk_text_layout_iter_starts_line:
#: gtktextlayout.c:3384:1
msgid "@iter: iterator to test"
msgstr ""

# #used by: gtk_text_layout_iter_starts_line:
#: gtktextlayout.c:3386:1
msgid "Tests whether an iterator is at the start of a display line."
msgstr ""

# used by: gtk_text_layout_move_iter_to_x:
#: gtktextlayout.c:3445:1
msgid "@x:      X coordinate"
msgstr ""

# #used by: gtk_text_layout_move_iter_to_x:
#: gtktextlayout.c:3447:3
msgid ""
"Keeping the iterator on the same line of the layout, move it to the "
"specified X coordinate. The lines of a wrapped paragraph are treated as "
"distinct for this operation."
msgstr ""

# used by: gtk_text_layout_move_iter_visually:
#: gtktextlayout.c:3502:1
msgid "@layout:  a #GtkTextLayout"
msgstr ""

# used by: gtk_text_layout_move_iter_visually:
#: gtktextlayout.c:3503:1
msgid "@iter:    a #GtkTextIter"
msgstr ""

# used by: gtk_text_layout_move_iter_visually:
#: gtktextlayout.c:3504:1
msgid ""
"@count:   number of characters to move (negative moves left, positive moves "
"right)"
msgstr ""

# #used by: gtk_text_layout_move_iter_visually:
#: gtktextlayout.c:3506:6
msgid ""
"Move the iterator a given number of characters visually, treating it as the "
"strong cursor position. If @count is positive, then the new strong cursor "
"position will be @count positions to the right of the old cursor position. "
"If @count is negative then the new strong cursor position will be @count "
"positions to the left of the old cursor position."
msgstr ""

# #used by: gtk_text_layout_move_iter_visually:
#: gtktextlayout.c:3513:4
msgid ""
"In the presence of bidirection text, the correspondence between logical and "
"visual order will depend on the direction of the current run, and there may "
"be jumps when the cursor is moved off of the end of a run."
msgstr ""

# used by: gtk_text_layout_set_overwrite_mode:
#: gtktextlayout.c:417:1
msgid "@overwrite: overwrite mode"
msgstr ""

# #used by: gtk_text_layout_set_overwrite_mode:
#: gtktextlayout.c:419:1
msgid "Sets overwrite mode"
msgstr ""

# used by: gtk_text_layout_set_cursor_direction:
#: gtktextlayout.c:435:4
msgid ""
"@direction: the new direction(s) for which to draw cursors. "
"%GTK_TEXT_DIR_NONE means draw cursors for both left-to-right insertion and "
"right-to-left insertion. (The two cursors will be visually distinguished.)"
msgstr ""

# #used by: gtk_text_layout_set_cursor_direction:
#: gtktextlayout.c:440:5
msgid ""
"Sets which text directions (left-to-right and/or right-to-left) for which "
"cursors will be drawn for the insertion point. The visual point at which new "
"text is inserted depends on whether the new text is right-to-left or left-to-"
"right, so it may be desired to make the drawn position of the cursor depend "
"on the keyboard state."
msgstr ""

# used by: gtk_text_layout_set_keyboard_direction:
#: gtktextlayout.c:459:1
msgid "@keyboard_dir: the current direction of the keyboard."
msgstr ""

# #used by: gtk_text_layout_set_keyboard_direction:
#: gtktextlayout.c:461:3
msgid ""
"Sets the keyboard direction; this is used as for the bidirectional base "
"direction for the line with the cursor if the line contains only neutral "
"characters."
msgstr ""

# #used by: gtk_text_layout_get_buffer:
#: gtktextlayout.c:480:2
msgid ""
"Gets the text buffer used by the layout. See gtk_text_layout_set_buffer()."
msgstr ""

# #used by: gtk_text_layout_get_buffer:
#: gtktextlayout.c:483:1
msgid "Return value: the text buffer used by the layout."
msgstr ""

# used by: gtk_text_layout_set_cursor_visible:
#: gtktextlayout.c:512:2
msgid ""
"@cursor_visible: If %FALSE, then the insertion cursor will not be shown, "
"even if the text is editable."
msgstr ""

# #used by: gtk_text_layout_set_cursor_visible:
#: gtktextlayout.c:515:3
msgid ""
"Sets whether the insertion cursor should be shown. Generally, widgets using "
"#GtkTextLayout will hide the cursor when the widget does not have the input "
"focus."
msgstr ""

# #used by: gtk_text_layout_get_cursor_visible:
#: gtktextlayout.c:548:1
msgid "Returns whether the insertion cursor will be shown."
msgstr ""

# #used by: gtk_text_layout_get_cursor_visible:
#: gtktextlayout.c:550:2
msgid ""
"Return value: if %FALSE, the insertion cursor will not be shown, even if the "
"text is editable."
msgstr ""

# used by: gtk_text_layout_set_preedit_string:
#: gtktextlayout.c:561:1
msgid "@layout: a #PangoLayout"
msgstr ""

# used by: gtk_text_layout_set_preedit_string:
#: gtktextlayout.c:562:1
msgid "@preedit_string: a string to display at the insertion point"
msgstr ""

# used by: gtk_text_layout_set_preedit_string:
#: gtktextlayout.c:563:1
msgid ""
"@preedit_attrs: a #PangoAttrList of attributes that apply to @preedit_string"
msgstr ""

# used by: gtk_text_layout_set_preedit_string:
#: gtktextlayout.c:564:1
msgid "@cursor_pos: position of cursor within preedit string in chars"
msgstr ""

# #used by: gtk_text_layout_set_preedit_string:
#: gtktextlayout.c:566:3
msgid ""
"Set the preedit string and attributes. The preedit string is a string "
"showing text that is currently being edited and not yet committed into the "
"buffer."
msgstr ""

