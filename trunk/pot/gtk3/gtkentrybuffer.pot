# used by: GtkEntryBuffer::inserted-text:
#: gtkentrybuffer.c:391:1
msgid "@chars: The text that was inserted."
msgstr ""

# used by: GtkEntryBuffer::inserted-text:
#: gtkentrybuffer.c:392:1
msgid "@n_chars: The number of characters that were inserted."
msgstr ""

# #used by: GtkEntryBuffer::inserted-text:
#: gtkentrybuffer.c:394:1
