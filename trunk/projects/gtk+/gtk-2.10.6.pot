#: gtktextbuffer.c:2650:1
msgid "@byte_index: byte index from start of line"
msgstr ""

# #used by: gtk_text_buffer_get_iter_at_line_index:
#: gtktextbuffer.c:2652:4
msgid ""
"Obtains an iterator pointing to @byte_index within the given line. "
"@byte_index must be the start of a UTF-8 character, and must not be beyond "
"the end of the line.  Note <emphasis>bytes</emphasis>, not characters; UTF-8 "
"may encode one character as multiple bytes."
msgstr ""

# #used by: gtk_text_buffer_get_iter_at_line:
#: gtktextbuffer.c:2677:1
msgid "Initializes @iter to the start of the given line."
msgstr ""

# used by: gtk_text_buffer_get_iter_at_offset:
#: gtktextbuffer.c:2694:1
msgid "@char_offset: char offset from start of buffer, counting from 0, or -1"
msgstr ""

# #used by: gtk_text_buffer_get_iter_at_offset:
#: gtktextbuffer.c:2696:4
msgid ""
"Initializes @iter to a position @char_offset chars from the start of the "
"entire buffer. If @char_offset is -1 or greater than the number of "
"characters in the buffer, @iter is initialized to the end iterator, the "
"iterator one past the last valid character in the buffer."
msgstr ""

# #used by: gtk_text_buffer_get_start_iter:
#: gtktextbuffer.c:2717:3
msgid ""
"Initialized @iter with the first position in the text buffer. This is the "
"same as using gtk_text_buffer_get_iter_at_offset() to get the iter at "
"character offset 0."
msgstr ""

# #used by: gtk_text_buffer_get_end_iter:
#: gtktextbuffer.c:2736:6
msgid ""
"Initializes @iter with the |$_1_$|end iterator,|$_1_$| one past the last "
"valid character in the text buffer. If dereferenced with "
"gtk_text_iter_get_char(), the end iterator has a character value of 0. The "
"entire buffer lies in the range from the first position in the buffer (call "
"gtk_text_buffer_get_start_iter() to get character position 0) to the end "
"iterator."
msgstr ""

# used by: gtk_text_buffer_get_bounds:
#: gtktextbuffer.c:2757:1
msgid "@start: iterator to initialize with first position in the buffer"
msgstr ""

# used by: gtk_text_buffer_get_bounds:
#: gtktextbuffer.c:2758:1
msgid "@end: iterator to initialize with the end iterator"
msgstr ""

# #used by: gtk_text_buffer_get_bounds:
#: gtktextbuffer.c:2760:2
msgid ""
"Retrieves the first and last iterators in the buffer, i.e. the entire buffer "
"lies within the range [@start,@end)."
msgstr ""

# #used by: gtk_text_buffer_get_modified:
#: gtktextbuffer.c:2785:4
msgid ""
"Indicates whether the buffer has been modified since the last call to "
"gtk_text_buffer_set_modified() set the modification flag to %FALSE. Used for "
"example to enable a |$_1_$|save|$_1_$| function in a text editor."
msgstr ""

# #used by: gtk_text_buffer_get_modified:
#: gtktextbuffer.c:2790:1
msgid "Return value: %TRUE if the buffer has been modified"
msgstr ""

# used by: gtk_text_buffer_set_modified:
#: gtktextbuffer.c:2803:1
msgid "@setting: modification flag setting"
msgstr ""

# #used by: gtk_text_buffer_set_modified:
#: gtktextbuffer.c:2805:5
msgid ""
"Used to keep track of whether the buffer has been modified since the last "
"time it was saved. Whenever the buffer is saved to disk, call "
"gtk_text_buffer_set_modified (@buffer, FALSE). When the buffer is modified, "
"it will automatically toggled on the modified bit again. When the modified "
"bit flips, the buffer emits a |$_1_$|modified_changed|$_1_$| signal."
msgstr ""

# #used by: gtk_text_buffer_get_has_selection:
#: gtktextbuffer.c:2835:1
msgid "Indicates whether the buffer has some text currently selected."
msgstr ""

# #used by: gtk_text_buffer_get_has_selection:
#: gtktextbuffer.c:2837:1
msgid "Return value: %TRUE if the there is text selected"
msgstr ""

# #used by: gtk_text_buffer_get_line_count:
#: gtktextbuffer.c:2858:2
msgid ""
"Obtains the number of lines in the buffer. This value is cached, so the "
"function is very fast."
msgstr ""

# #used by: gtk_text_buffer_get_line_count:
#: gtktextbuffer.c:2861:1
msgid "Return value: number of lines in the buffer"
msgstr ""

# #used by: gtk_text_buffer_get_char_count:
#: gtktextbuffer.c:2875:4
msgid ""
"Gets the number of characters in the buffer; note that characters and bytes "
"are not the same, you can't e.g. expect the contents of the buffer in string "
"form to be this many bytes long. The character count is cached, so this "
"function is very fast."
msgstr ""

# #used by: gtk_text_buffer_get_char_count:
#: gtktextbuffer.c:2880:1
msgid "Return value: number of characters in the buffer"
msgstr ""

# #used by: gtk_text_buffer_add_selection_clipboard:
#: gtktextbuffer.c:3448:3
msgid ""
"Adds @clipboard to the list of clipboards in which the selection contents of "
"@buffer are available. In most cases, @clipboard will be the #GtkClipboard "
"of type %GDK_SELECTION_PRIMARY for a view of @buffer."
msgstr ""

# used by: gtk_text_buffer_remove_selection_clipboard:
#: gtktextbuffer.c:3480:1
msgid ""
"@clipboard: a #GtkClipboard added to @buffer by "
"gtk_text_buffer_add_selection_clipboard()."
msgstr ""

# #used by: gtk_text_buffer_remove_selection_clipboard:
#: gtktextbuffer.c:3482:1
msgid ""
"Removes a #GtkClipboard added with gtk_text_buffer_add_selection_clipboard()"
msgstr ""

# used by: gtk_text_buffer_paste_clipboard:
#: gtktextbuffer.c:3520:1
msgid "@clipboard: the #GtkClipboard to paste from"
msgstr ""

# used by: gtk_text_buffer_paste_clipboard:
#: gtktextbuffer.c:3521:1
msgid ""
"@override_location: location to insert pasted text, or %NULL for at the "
"cursor"
msgstr ""

# #used by: gtk_text_buffer_paste_clipboard:
#: gtktextbuffer.c:3524:4
msgid ""
"Pastes the contents of a clipboard at the insertion point, or at "
"@override_location. (Note: pasting is asynchronous, that is, we'll ask for "
"the paste data and return, and at some point later after the main loop runs, "
"the paste data will be inserted.)"
msgstr ""

# #-#-#-#-#  gtktextbuffer.pot~ (gtk 2.10.6)  #-#-#-#-#
# used by: gtk_text_buffer_delete_selection:
# #-#-#-#-#  gtktextbuffer.pot~ (gtk 2.10.6)  #-#-#-#-#
# used by: gtk_text_buffer_backspace:
#: gtktextbuffer.c:3571:1 gtktextbuffer.c:3612:1
msgid "@interactive: whether the deletion is caused by user interaction"
msgstr ""

# #used by: gtk_text_buffer_delete_selection:
#: gtktextbuffer.c:3574:4
msgid ""
"Deletes the range between the |$_1_$|insert|$_1_$| and |$_1_$|"
"selection_bound|$_1_$| marks, that is, the currently-selected text. If "
"@interactive is %TRUE, the editability of the selection will be considered "
"(users can't delete uneditable text)."
msgstr ""

# #used by: gtk_text_buffer_delete_selection:
#: gtktextbuffer.c:3579:1
msgid "Return value: whether there was a non-empty selection to delete"
msgstr ""

# #used by: gtk_text_buffer_backspace:
#: gtktextbuffer.c:3615:6
msgid ""
"Performs the appropriate action as if the user hit the delete key with the "
"cursor at the position specified by @iter. In the normal case a single "
"character will be deleted, but when combining accents are involved, more "
"than one character can be deleted, and when precomposed character and accent "
"combinations are involved, less than one character will be deleted."
msgstr ""

# #used by: gtk_text_buffer_backspace:
#: gtktextbuffer.c:3622:3
msgid ""
"Because the buffer is modified, all outstanding iterators become invalid "
"after calling this function; however, the @iter will be re-initialized to "
"point to the location where text was deleted."
msgstr ""

# #used by: gtk_text_buffer_backspace:
#: gtktextbuffer.c:3626:1
msgid "Return value: %TRUE if the buffer was modified"
msgstr ""

# used by: gtk_text_buffer_cut_clipboard:
#: gtktextbuffer.c:3781:1
msgid "@clipboard: the #GtkClipboard object to cut to."
msgstr ""

# #used by: gtk_text_buffer_cut_clipboard:
#: gtktextbuffer.c:3784:2
msgid ""
"Copies the currently-selected text to a clipboard, then deletes said text if "
"it's editable."
msgstr ""

# used by: gtk_text_buffer_copy_clipboard:
#: gtktextbuffer.c:3801:1
msgid "@clipboard: the #GtkClipboard object to copy to."
msgstr ""

# #used by: gtk_text_buffer_copy_clipboard:
#: gtktextbuffer.c:3803:1
msgid "Copies the currently-selected text to a clipboard."
msgstr ""

# used by: gtk_text_buffer_get_selection_bounds:
#: gtktextbuffer.c:3818:1
msgid "@buffer: a #GtkTextBuffer a #GtkTextBuffer"
msgstr ""

# used by: gtk_text_buffer_get_selection_bounds:
#: gtktextbuffer.c:3819:1
msgid "@start: iterator to initialize with selection start"
msgstr ""

# used by: gtk_text_buffer_get_selection_bounds:
#: gtktextbuffer.c:3820:1
msgid "@end: iterator to initialize with selection end"
msgstr ""

# #used by: gtk_text_buffer_get_selection_bounds:
#: gtktextbuffer.c:3822:6
msgid ""
"Returns %TRUE if some text is selected; places the bounds of the selection "
"in @start and @end (if the selection has length 0, then @start and @end are "
"filled in with the same value). @start and @end will be in ascending order. "
"If @start and @end are NULL, then they are not filled in, but the return "
"value still indicates whether text is selected."
msgstr ""

# #used by: gtk_text_buffer_get_selection_bounds:
#: gtktextbuffer.c:3829:1
msgid "Return value: whether the selection has nonzero length"
msgstr ""

# #used by: gtk_text_buffer_begin_user_action:
#: gtktextbuffer.c:3845:10
msgid ""
"Called to indicate that the buffer operations between here and a call to "
"gtk_text_buffer_end_user_action() are part of a single user-visible "
"operation. The operations between gtk_text_buffer_begin_user_action() and "
"gtk_text_buffer_end_user_action() can then be grouped when creating an undo "
"stack. #GtkTextBuffer maintains a count of calls to "
"gtk_text_buffer_begin_user_action() that have not been closed with a call to "
"gtk_text_buffer_end_user_action(), and emits the |$_1_$|begin_user_action|"
"$_1_$| and |$_1_$|end_user_action|$_1_$| signals only for the outermost pair "
"of calls. This allows you to build user actions from other user actions."
msgstr ""

# #used by: gtk_text_buffer_begin_user_action:
#: gtktextbuffer.c:3856:5
msgid ""
"The |$_1_$|interactive|$_1_$| buffer mutation functions, such as "
"gtk_text_buffer_insert_interactive(), automatically call begin/end user "
"action around the buffer operations they perform, so there's no need to add "
"extra calls if you user action consists solely of a single call to one of "
"those functions."
msgstr ""

# #used by: gtk_text_buffer_end_user_action:
#: gtktextbuffer.c:3880:2
msgid ""
"Should be paired with a call to gtk_text_buffer_begin_user_action(). See "
"that function for a full explanation."
msgstr ""

# #used by: gtk_text_buffer_get_copy_target_list:
