msgstr ""

#. used by: GtkPrintOperationResult:
#: gtkprintoperation.h:87:1
msgid "@GTK_PRINT_OPERATION_RESULT_APPLY: The print settings should be stored."
msgstr ""

#. used by: GtkPrintOperationResult:
#: gtkprintoperation.h:88:2
msgid ""
"@GTK_PRINT_OPERATION_RESULT_CANCEL: The print operation has been canceled, "
"the print settings should not be stored."
msgstr ""

#. used by: GtkPrintOperationResult:
