#: gtktextiter.c:3706:1
msgid "@count: number of positions to move"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_cursor_positions:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_cursor_positions:
#: gtktextiter.c:3615:2 gtktextiter.c:3634:2
msgid ""
"Moves up to @count cursor positions. See "
"gtk_text_iter_forward_cursor_position() for details."
msgstr ""

#. used by: gtk_text_iter_forward_visible_cursor_position:
#: gtktextiter.c:3652:2
msgid ""
"Moves @iter forward to the next visible cursor position. See "
"gtk_text_iter_forward_cursor_position() for details."
msgstr ""

#. used by: gtk_text_iter_backward_visible_cursor_position:
#: gtktextiter.c:3669:2
msgid ""
"Moves @iter forward to the previous visible cursor position. See "
"gtk_text_iter_backward_cursor_position() for details."
msgstr ""

#. used by: gtk_text_iter_forward_visible_cursor_positions:
#: gtktextiter.c:3687:2
msgid ""
"Moves up to @count visible cursor positions. See "
"gtk_text_iter_forward_cursor_position() for details."
msgstr ""

#. used by: gtk_text_iter_backward_visible_cursor_positions:
#: gtktextiter.c:3708:2
msgid ""
"Moves up to @count visible cursor positions. See "
"gtk_text_iter_backward_cursor_position() for details."
msgstr ""

#. used by: gtk_text_iter_is_cursor_position:
#: gtktextiter.c:3728:2
msgid ""
"See gtk_text_iter_forward_cursor_position() or #PangoLogAttr or pango_break"
"() for details on what a cursor position is."
msgstr ""

#. used by: gtk_text_iter_is_cursor_position:
#: gtktextiter.c:3731:1
msgid "Return value: %TRUE if the cursor can be placed at @iter"
msgstr ""

#. used by: gtk_text_iter_set_line_offset:
#: gtktextiter.c:3742:1
msgid ""
"@char_on_line: a character offset relative to the start of @iter's current "
"line"
msgstr ""

#. used by: gtk_text_iter_set_line_offset:
#: gtktextiter.c:3744:6
msgid ""
"Moves @iter within a line, to a new <emphasis>character</emphasis> (not "
"byte) offset. The given character offset must be less than or equal to the "
"number of characters in the line; if equal, @iter moves to the start of the "
"next line. See gtk_text_iter_set_line_index() if you have a byte index "
"rather than a character offset."
msgstr ""

#. used by: gtk_text_iter_set_line_index:
#: gtktextiter.c:3783:1
msgid ""
"@byte_on_line: a byte index relative to the start of @iter's current line"
msgstr ""

#. used by: gtk_text_iter_set_line_index:
#: gtktextiter.c:3785:4
msgid ""
"Same as gtk_text_iter_set_line_offset(), but works with a <emphasis>byte</"
"emphasis> index. The given byte index must be at the start of a character, "
"it can't be in the middle of a UTF-8 encoded character."
msgstr ""

#. used by: gtk_text_iter_set_visible_line_offset:
#: gtktextiter.c:3830:1
msgid "@char_on_line: a character offset"
msgstr ""

#. used by: gtk_text_iter_set_visible_line_offset:
#: gtktextiter.c:3832:3
msgid ""
"Like gtk_text_iter_set_line_offset(), but the offset is in visible "
"characters, i.e. text with a tag making it invisible is not counted in the "
"offset."
msgstr ""

#. used by: gtk_text_iter_get_buffer:
#: gtktextiter.c:384:1
msgid "Returns the #GtkTextBuffer this iterator is associated with."
msgstr ""

#. used by: gtk_text_iter_get_buffer:
#: gtktextiter.c:386:1
msgid "Return value: (transfer none): the buffer"
msgstr ""

#. used by: gtk_text_iter_set_visible_line_index:
#: gtktextiter.c:3871:1
msgid "@byte_on_line: a byte index"
msgstr ""

#. used by: gtk_text_iter_set_visible_line_index:
#: gtktextiter.c:3873:3
msgid ""
"Like gtk_text_iter_set_line_index(), but the index is in visible bytes, i.e. "
"text with a tag making it invisible is not counted in the index."
msgstr ""

#. used by: gtk_text_iter_set_line:
#: gtktextiter.c:3931:1
msgid "@line_number: line number (counted from 0)"
msgstr ""

#. used by: gtk_text_iter_set_line:
#: gtktextiter.c:3933:3
msgid ""
"Moves iterator @iter to the start of the line @line_number.  If @line_number "
"is negative or larger than the number of lines in the buffer, moves @iter to "
"the start of the last line in the buffer."
msgstr ""

#. used by: gtk_text_iter_set_offset:
#: gtktextiter.c:3968:1
msgid "@char_offset: a character number"
msgstr ""

#. used by: gtk_text_iter_set_offset:
#: gtktextiter.c:3970:2
msgid ""
"Sets @iter to point to @char_offset. @char_offset counts from the start of "
"the entire text buffer, starting with 0."
msgstr ""

#. used by: SECTION:gtktextiter
#: gtktextiter.c:40:1
msgid "@Short_description: Text buffer iterator"
msgstr ""

#. used by: gtk_text_iter_forward_to_end:
#: gtktextiter.c:4012:3
msgid ""
"Moves @iter forward to the \"end iterator,\" which points one past the last "
"valid character in the buffer. gtk_text_iter_get_char() called on the end "
"iterator returns 0, which is convenient for writing loops."
msgstr ""

#. used by: gtk_text_iter_forward_to_line_end:
#: gtktextiter.c:4068:8
msgid ""
"Moves the iterator to point to the paragraph delimiter characters, which "
"will be either a newline, a carriage return, a carriage return/newline in "
"sequence, or the Unicode paragraph separator character. If the iterator is "
"already at the paragraph delimiter characters, moves to the paragraph "
"delimiter characters for the next line. If @iter is on the last line in the "
"buffer, which does not end in paragraph delimiters, moves to the end "
"iterator (end of the last line), and returns %FALSE."
msgstr ""

#. used by: gtk_text_iter_forward_to_line_end:
#: gtktextiter.c:4077:1
msgid ""
"Return value: %TRUE if we moved and the new location is not the end iterator"
msgstr ""

#. used by: gtk_text_iter_copy:
#: gtktextiter.c:409:4
msgid ""
"Creates a dynamically-allocated copy of an iterator. This function is not "
"useful in applications, because iterators can be copied with a simple "
"assignment (<literal>GtkTextIter i = j;</literal>). The function is used by "
"language bindings."
msgstr ""

#. used by: SECTION:gtktextiter
#: gtktextiter.c:41:1
msgid "@Title: GtkTextIter"
msgstr ""

#. used by: gtk_text_iter_forward_to_tag_toggle:
#: gtktextiter.c:4119:7
msgid ""
"Moves forward to the next toggle (on or off) of the #GtkTextTag @tag, or to "
"the next toggle of any tag if @tag is %NULL. If no matching tag toggles are "
"found, returns %FALSE, otherwise %TRUE. Does not return toggles located at "
"@iter, only toggles after @iter. Sets @iter to the location of the toggle, "
"or to the end of the buffer if no toggle is found."
msgstr ""

#. used by: gtk_text_iter_forward_to_tag_toggle:
#: gtktextiter.c:4127:1
msgid "Return value: whether we found a tag toggle after @iter"
msgstr ""

#. used by: gtk_text_iter_copy:
#: gtktextiter.c:414:1
msgid "Return value: a copy of the @iter, free with gtk_text_iter_free ()"
msgstr ""

#. used by: gtk_text_iter_backward_to_tag_toggle:
#: gtktextiter.c:4201:7
msgid ""
"Moves backward to the next toggle (on or off) of the #GtkTextTag @tag, or to "
"the next toggle of any tag if @tag is %NULL. If no matching tag toggles are "
"found, returns %FALSE, otherwise %TRUE. Does not return toggles located at "
"@iter, only toggles before @iter. Sets @iter to the location of the toggle, "
"or the start of the buffer if no toggle is found."
msgstr ""

#. used by: gtk_text_iter_backward_to_tag_toggle:
#: gtktextiter.c:4209:1
msgid "Return value: whether we found a tag toggle before @iter"
msgstr ""

#. used by: SECTION:gtktextiter
#: gtktextiter.c:43:3
msgid ""
"You may wish to begin by reading the <link linkend=\"TextWidget\">text "
"widget conceptual overview</link> which gives an overview of all the objects "
"and data types related to the text widget and how they work together."
msgstr ""

#. used by: gtk_text_iter_forward_find_char:
#: gtktextiter.c:4312:1
msgid "@pred: (scope call): a function to be called on each character"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_find_char:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_find_char:
#: gtktextiter.c:4313:1 gtktextiter.c:4351:1
msgid "@user_data: user data for @pred"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_find_char:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_find_char:
#: gtktextiter.c:4314:1 gtktextiter.c:4352:1
msgid "@limit: (allow-none): search limit, or %NULL for none"
msgstr ""

#. used by: gtk_text_iter_forward_find_char:
#: gtktextiter.c:4316:4
msgid ""
"Advances @iter, calling @pred on each character. If @pred returns %TRUE, "
"returns %TRUE and stops scanning. If @pred never returns %TRUE, @iter is set "
"to @limit if @limit is non-%NULL, otherwise to the end iterator."
msgstr ""

#. used by: gtk_text_iter_free:
#: gtktextiter.c:432:1
msgid "@iter: a dynamically-allocated iterator"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_find_char:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_find_char:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_search:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:4321:1 gtktextiter.c:4356:1 gtktextiter.c:4850:1
#: gtktextiter.c:5171:1
msgid "Return value: whether a match was found"
msgstr ""

#. used by: gtk_text_iter_free:
#: gtktextiter.c:434:4
msgid ""
"Free an iterator allocated on the heap. This function is intended for use in "
"language bindings, and is not especially useful for applications, because "
"iterators can simply be allocated on the stack."
msgstr ""

#. used by: gtk_text_iter_backward_find_char:
#: gtktextiter.c:4350:1
msgid "@pred: (scope call): function to be called on each character"
msgstr ""

#. used by: gtk_text_iter_backward_find_char:
#: gtktextiter.c:4354:1
msgid ""
"Same as gtk_text_iter_forward_find_char(), but goes backward from @iter."
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4826:1
msgid "@iter: start of search"
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4827:1
msgid "@str: a search string"
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4828:1
msgid "@flags: flags affecting how the search is done"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_search:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:4829:1 gtktextiter.c:5165:1
msgid ""
"@match_start: (out caller-allocates) (allow-none): return location for start "
"of match, or %NULL"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_forward_search:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:4830:1 gtktextiter.c:5166:1
msgid ""
"@match_end: (out caller-allocates) (allow-none): return location for end of "
"match, or %NULL"
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4831:1
msgid ""
"@limit: (allow-none): bound for the search, or %NULL for the end of the "
"buffer"
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4833:6
msgid ""
"Searches forward for @str. Any match is returned by setting @match_start to "
"the first character of the match and @match_end to the first character after "
"the match. The search will not continue past @limit. Note that a search is a "
"linear or O(n) operation, so you may wish to use @limit to avoid locking up "
"your UI on large buffers."
msgstr ""

#. used by: gtk_text_iter_forward_search:
#: gtktextiter.c:4840:9
msgid ""
"If the #GTK_TEXT_SEARCH_VISIBLE_ONLY flag is present, the match may have "
"invisible text interspersed in @str. i.e. @str will be a possibly-"
"noncontiguous subsequence of the matched range. similarly, if you specify "
"#GTK_TEXT_SEARCH_TEXT_ONLY, the match may have pixbufs or child widgets "
"mixed inside the matched range. If these flags are not given, the match must "
"be exact; the special 0xFFFC character in @str will match embedded pixbufs "
"or child widgets. If you specify the #GTK_TEXT_SEARCH_CASE_INSENSITIVE flag, "
"the text will be matched regardless of what case it is in."
msgstr ""

#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:5162:1
msgid "@iter: a #GtkTextIter where the search begins"
msgstr ""

#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:5163:1
msgid "@str: search string"
msgstr ""

#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:5164:1
msgid "@flags: bitmask of flags affecting the search"
msgstr ""

#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:5167:1
msgid ""
"@limit: (allow-none): location of last possible @match_start, or %NULL for "
"start of buffer"
msgstr ""

#. used by: gtk_text_iter_backward_search:
#: gtktextiter.c:5169:1
msgid "Same as gtk_text_iter_forward_search(), but moves backward."
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_equal:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_compare:
#: gtktextiter.c:5311:1 gtktextiter.c:5352:1
msgid "@lhs: a #GtkTextIter"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_equal:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_compare:
#: gtktextiter.c:5312:1 gtktextiter.c:5353:1
msgid "@rhs: another #GtkTextIter"
msgstr ""

#. used by: gtk_text_iter_equal:
#: gtktextiter.c:5314:5
msgid ""
"Tests whether two iterators are equal, using the fastest possible mechanism. "
"This function is very fast; you can expect it to perform better than e.g. "
"getting the character offset for each iterator and comparing the offsets "
"yourself. Also, it's a bit faster than gtk_text_iter_compare()."
msgstr ""

#. used by: gtk_text_iter_equal:
#: gtktextiter.c:5320:1
msgid ""
"Return value: %TRUE if the iterators point to the same place in the buffer"
msgstr ""

#. used by: gtk_text_iter_compare:
#: gtktextiter.c:5355:4
msgid ""
"A qsort()-style function that returns negative if @lhs is less than @rhs, "
"positive if @lhs is greater than @rhs, and 0 if they're equal. Ordering is "
"in character offset order, i.e. the first character in the buffer is less "
"than the second character in the buffer."
msgstr ""

#. used by: gtk_text_iter_compare:
#: gtktextiter.c:5360:1
msgid ""
"Return value: -1 if @lhs is less than @rhs, 1 if @lhs is greater, 0 if they "
"are equal"
msgstr ""

#. used by: gtk_text_iter_in_range:
#: gtktextiter.c:5424:1
msgid "@start: start of range"
msgstr ""

#. used by: gtk_text_iter_in_range:
#: gtktextiter.c:5425:1
msgid "@end: end of range"
msgstr ""

#. used by: gtk_text_iter_in_range:
#: gtktextiter.c:5427:2
msgid ""
"Checks whether @iter falls in the range [@start, @end). @start and @end must "
"be in ascending order."
msgstr ""

#. used by: gtk_text_iter_in_range:
#: gtktextiter.c:5430:1
msgid "Return value: %TRUE if @iter is in the range"
msgstr ""

#. used by: gtk_text_iter_order:
#: gtktextiter.c:5448:1
msgid "@first: a #GtkTextIter"
msgstr ""

#. used by: gtk_text_iter_order:
#: gtktextiter.c:5449:1
msgid "@second: another #GtkTextIter"
msgstr ""

#. used by: gtk_text_iter_order:
#: gtktextiter.c:5451:6
msgid ""
"Swaps the value of @first and @second if @second comes before @first in the "
"buffer. That is, ensures that @first and @second are in sequence. Most text "
"buffer functions that take a range call this automatically on your behalf, "
"so there's no real reason to call it yourself in those cases. There are some "
"exceptions, such as gtk_text_iter_in_range(), that expect a pre-sorted range."
msgstr ""

#. used by: gtk_text_iter_get_offset:
#: gtktextiter.c:563:5
msgid ""
"Returns the character offset of an iterator. Each character in a "
"#GtkTextBuffer has an offset, starting with 0 for the first character in the "
"buffer. Use gtk_text_buffer_get_iter_at_offset () to convert an offset back "
"into an iterator."
msgstr ""

#. used by: gtk_text_iter_get_offset:
#: gtktextiter.c:569:1
msgid "Return value: a character offset"
msgstr ""

#. used by: gtk_text_iter_get_line:
#: gtktextiter.c:603:3
msgid ""
"Returns the line number containing the iterator. Lines in a #GtkTextBuffer "
"are numbered beginning with 0 for the first line in the buffer."
msgstr ""

#. used by: gtk_text_iter_get_line:
#: gtktextiter.c:607:1
msgid "Return value: a line number"
msgstr ""

#. used by: gtk_text_iter_get_line_offset:
#: gtktextiter.c:634:3
msgid ""
"Returns the character offset of the iterator, counting from the start of a "
"newline-terminated line. The first character on the line has offset 0."
msgstr ""

#. used by: gtk_text_iter_get_line_offset:
#: gtktextiter.c:638:1
msgid "Return value: offset from start of line"
msgstr ""

#. used by: gtk_text_iter_get_line_index:
#: gtktextiter.c:663:5
msgid ""
"Returns the byte index of the iterator, counting from the start of a newline-"
"terminated line. Remember that #GtkTextBuffer encodes text in UTF-8, and "
"that characters can require a variable number of bytes to represent."
msgstr ""

#. used by: gtk_text_iter_get_line_index:
#: gtktextiter.c:669:1
msgid "Return value: distance from start of line, in bytes"
msgstr ""

#. used by: gtk_text_iter_get_visible_line_offset:
#: gtktextiter.c:694:4
msgid ""
"Returns the offset in characters from the start of the line to the given "
"@iter, not counting characters that are invisible due to tags with the "
"\"invisible\" flag toggled on."
msgstr ""

#. used by: gtk_text_iter_get_visible_line_offset:
#: gtktextiter.c:699:1
msgid "Return value: offset in visible characters from the start of the line"
msgstr ""

#. used by: gtk_text_iter_get_visible_line_index:
#: gtktextiter.c:759:4
msgid ""
"Returns the number of bytes from the start of the line to the given @iter, "
"not counting bytes that are invisible due to tags with the \"invisible\" "
"flag toggled on."
msgstr ""

#. used by: gtk_text_iter_get_visible_line_index:
#: gtktextiter.c:764:1
msgid "Return value: byte index of @iter with respect to the start of the line"
msgstr ""

#. used by: gtk_text_iter_get_char:
#: gtktextiter.c:827:7
msgid ""
"Returns the Unicode character at this iterator.  (Equivalent to operator* on "
"a C++ iterator.)  If the element at this iterator is a non-character "
"element, such as an image embedded in the buffer, the Unicode \"unknown\" "
"character 0xFFFC is returned. If invoked on the end iterator, zero is "
"returned; zero is not a valid Unicode character. So you can write a loop "
"which ends when gtk_text_iter_get_char () returns 0."
msgstr ""

#. used by: gtk_text_iter_get_char:
#: gtktextiter.c:835:1
msgid "Return value: a Unicode character, or 0 if @iter is not dereferenceable"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_slice:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_text:
#: gtktextiter.c:869:1 gtktextiter.c:898:1
msgid "@start: iterator at start of a range"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_slice:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_text:
#: gtktextiter.c:870:1 gtktextiter.c:899:1
msgid "@end: iterator at end of a range"
msgstr ""

#. used by: gtk_text_iter_get_slice:
#: gtktextiter.c:872:8
msgid ""
"Returns the text in the given range. A \"slice\" is an array of characters "
"encoded in UTF-8 format, including the Unicode \"unknown\" character 0xFFFC "
"for iterable non-character elements in the buffer, such as images.  Because "
"images are encoded in the slice, byte and character offsets in the returned "
"array will correspond to byte offsets in the text buffer. Note that 0xFFFC "
"can occur in normal text as well, so it is not a reliable indicator that a "
"pixbuf or widget is in the buffer."
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_slice:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_visible_slice:
#: gtktextiter.c:881:1 gtktextiter.c:931:1
msgid "Return value: slice of text from the buffer"
msgstr ""

#. used by: gtk_text_iter_get_text:
#: gtktextiter.c:901:5
msgid ""
"Returns <emphasis>text</emphasis> in the given range.  If the range contains "
"non-text elements such as images, the character and byte offsets in the "
"returned string will not correspond to character and byte offsets in the "
"buffer. If you want offsets to correspond, see gtk_text_iter_get_slice ()."
msgstr ""

#. used by: gtk_text_iter_get_text:
#: gtktextiter.c:907:1
msgid "Return value: array of characters from the buffer"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_visible_slice:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_visible_text:
#: gtktextiter.c:924:1 gtktextiter.c:948:1
msgid "@start: iterator at start of range"
msgstr ""

#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_visible_slice:
#. #-#-#-#-#  gtktextiter.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_text_iter_get_visible_text:
#: gtktextiter.c:925:1 gtktextiter.c:949:1
msgid "@end: iterator at end of range"
msgstr ""

#. used by: gtk_text_iter_get_visible_slice:
#: gtktextiter.c:927:3
msgid ""
"Like gtk_text_iter_get_slice (), but invisible text is not included. "
"Invisible text is usually invisible because a #GtkTextTag with the "
"\"invisible\" attribute turned on has been applied to it."
msgstr ""

#. used by: gtk_text_iter_get_visible_text:
#: gtktextiter.c:951:3
msgid ""
"Like gtk_text_iter_get_text (), but invisible text is not included. "
"Invisible text is usually invisible because a #GtkTextTag with the "
"\"invisible\" attribute turned on has been applied to it."
msgstr ""

#. used by: gtk_text_iter_get_visible_text:
#: gtktextiter.c:955:1
msgid "Return value: string containing visible text in the range"
msgstr ""

#. used by: gtk_text_iter_get_pixbuf:
#: gtktextiter.c:974:3
msgid ""
"If the element at @iter is a pixbuf, the pixbuf is returned (with no new "
"reference count added). Otherwise, %NULL is returned."
msgstr ""

#. used by: gtk_text_iter_get_pixbuf:
#: gtktextiter.c:978:1
msgid "Return value: (transfer none): the pixbuf at @iter"
msgstr ""
