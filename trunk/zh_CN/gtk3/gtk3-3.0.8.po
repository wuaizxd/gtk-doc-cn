# #used by: gtk_drag_dest_set: (method)
#: gtkdnd.c:1236:8
msgid ""
"gdk_window_get_pointer (gtk_widget_get_window (widget), NULL, NULL, &mask); "
"if (mask & GDK_CONTROL_MASK) gdk_drag_status (context, GDK_ACTION_COPY, "
"time); else gdk_drag_status (context, GDK_ACTION_MOVE, time); } ]|"
