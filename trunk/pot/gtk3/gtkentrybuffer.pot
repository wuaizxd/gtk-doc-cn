#: gtkentrybuffer.c:436:1
msgid "@initial_chars: (allow-none): initial buffer text, or %NULL"
msgstr ""

# used by: gtk_entry_buffer_new:
#: gtkentrybuffer.c:437:1
msgid "@n_initial_chars: number of characters in @initial_chars, or -1"
msgstr ""

# #used by: gtk_entry_buffer_new:
#: gtkentrybuffer.c:439:1
msgid "Create a new GtkEntryBuffer object."
msgstr ""

# #used by: SECTION:gtkentrybuffer
#: gtkentrybuffer.c:44:4
msgid ""
"#GtkEntryBuffer may be derived from. Such a derived class might allow text "
"to be stored in an alternate location, such as non-pageable memory, useful "
"in the case of important passwords. Or a derived class could integrate with "
"an application's concept of undo/redo."
msgstr ""

# #used by: gtk_entry_buffer_new:
#: gtkentrybuffer.c:441:1
msgid "Optionally, specify initial text to set in the buffer."
msgstr ""

# #used by: gtk_entry_buffer_new:
#: gtkentrybuffer.c:443:1
msgid "Return value: A new GtkEntryBuffer object."
msgstr ""

# #used by: gtk_entry_buffer_get_length:
#: gtkentrybuffer.c:461:1
msgid "Retrieves the length in characters of the buffer."
msgstr ""

# #used by: gtk_entry_buffer_get_length:
#: gtkentrybuffer.c:463:1
msgid "Return value: The number of characters in the buffer."
msgstr ""

# #used by: gtk_entry_buffer_get_bytes:
#: gtkentrybuffer.c:484:2
msgid ""
"Retrieves the length in bytes of the buffer. See gtk_entry_buffer_get_length"
"()."
msgstr ""

# #used by: gtk_entry_buffer_get_bytes:
#: gtkentrybuffer.c:487:1
msgid "Return value: The byte length of the buffer."
msgstr ""

# #used by: gtk_entry_buffer_get_text:
#: gtkentrybuffer.c:510:1
msgid "Retrieves the contents of the buffer."
msgstr ""

# #used by: gtk_entry_buffer_get_text:
#: gtkentrybuffer.c:512:2
msgid ""
"The memory pointer returned by this call will not change unless this object "
"emits a signal, or is finalized."
msgstr ""

# #used by: gtk_entry_buffer_get_text:
#: gtkentrybuffer.c:515:4
msgid ""
"Return value: a pointer to the contents of the widget as a string. This "
"string points to internally allocated storage in the buffer and must not be "
"freed, modified or stored."
msgstr ""

# used by: gtk_entry_buffer_set_text:
#: gtkentrybuffer.c:538:1
msgid "@chars: the new text"
msgstr ""

# used by: gtk_entry_buffer_set_text:
#: gtkentrybuffer.c:539:1
msgid "@n_chars: the number of characters in @text, or -1"
msgstr ""

# #used by: gtk_entry_buffer_set_text:
#: gtkentrybuffer.c:541:1
msgid "Sets the text in the buffer."
msgstr ""

# #used by: gtk_entry_buffer_set_text:
#: gtkentrybuffer.c:543:2
msgid ""
"This is roughly equivalent to calling gtk_entry_buffer_delete_text() and "
"gtk_entry_buffer_insert_text()."
msgstr ""

# #used by: gtk_entry_buffer_set_text:
#: gtkentrybuffer.c:546:1
msgid "Note that @n_chars is in characters, not in bytes."
msgstr ""

# used by: gtk_entry_buffer_set_max_length:
#: gtkentrybuffer.c:567:3
msgid ""
"@max_length: the maximum length of the entry buffer, or 0 for no maximum. "
"(other than the maximum length of entries.) The value passed in will be "
"clamped to the range 0-65536."
msgstr ""

# #used by: gtk_entry_buffer_set_max_length:
#: gtkentrybuffer.c:571:3
msgid ""
"Sets the maximum allowed length of the contents of the buffer. If the "
"current contents are longer than the given length, then they will be "
"truncated to fit."
msgstr ""

# #used by: gtk_entry_buffer_get_max_length:
#: gtkentrybuffer.c:596:2
msgid ""
"Retrieves the maximum allowed length of the text in @buffer. See "
"gtk_entry_buffer_set_max_length()."
msgstr ""

# #used by: gtk_entry_buffer_get_max_length:
#: gtkentrybuffer.c:599:2
msgid ""
"Return value: the maximum allowed number of characters in #GtkEntryBuffer, "
"or 0 if there is no maximum."
msgstr ""

# used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:614:1
msgid "@position: the position at which to insert text."
msgstr ""

# used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:615:1
msgid "@chars: the text to insert into the buffer."
msgstr ""

# used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:616:1
msgid "@n_chars: the length of the text in characters, or -1"
msgstr ""

# #used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:618:2
msgid ""
"Inserts @n_chars characters of @chars into the contents of the buffer, at "
"position @position."
msgstr ""

# #used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:621:4
msgid ""
"If @n_chars is negative, then characters from chars will be inserted until a "
"null-terminator is found. If @position or @n_chars are out of bounds, or the "
"maximum buffer text length is exceeded, then they are coerced to sane values."
msgstr ""

# #used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:626:1
msgid "Note that the position and length are in characters, not in bytes."
msgstr ""

# #used by: gtk_entry_buffer_insert_text:
#: gtkentrybuffer.c:628:1
msgid "Returns: The number of characters actually inserted."
msgstr ""

# used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:672:1
msgid "@position: position at which to delete text"
msgstr ""

# used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:673:1
msgid "@n_chars: number of characters to delete"
msgstr ""

# #used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:675:3
msgid ""
"Deletes a sequence of characters from the buffer. @n_chars characters are "
"deleted starting at @position. If @n_chars is negative, then all characters "
"until the end of the text are deleted."
msgstr ""

# #used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:679:2
msgid ""
"If @position or @n_chars are out of bounds, then they are coerced to sane "
"values."
msgstr ""

# #used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:682:1
msgid "Note that the positions are specified in characters, not bytes."
msgstr ""

# #used by: gtk_entry_buffer_delete_text:
#: gtkentrybuffer.c:684:1
msgid "Returns: The number of characters deleted."
msgstr ""

# used by: gtk_entry_buffer_emit_inserted_text:
#: gtkentrybuffer.c:715:1
msgid "@position: position at which text was inserted"
msgstr ""

# used by: gtk_entry_buffer_emit_inserted_text:
#: gtkentrybuffer.c:716:1
msgid "@chars: text that was inserted"
msgstr ""

# used by: gtk_entry_buffer_emit_inserted_text:
#: gtkentrybuffer.c:717:1
msgid "@n_chars: number of characters inserted"
msgstr ""

