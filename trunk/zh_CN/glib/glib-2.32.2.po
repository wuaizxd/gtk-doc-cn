msgstr ""

#. used by: g_base64_encode:
#: gbase64.c:244:2
msgid ""
"Encode a sequence of binary data into its Base-64 stringified representation."
msgstr ""

#. used by: g_base64_encode:
#: gbase64.c:247:3
msgid ""
"Return value: (transfer full): a newly allocated, zero-terminated Base-64 "
"encoded string representing @data. The returned string must be freed with "
"g_free()."
msgstr ""

