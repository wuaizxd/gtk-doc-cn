#: ghash.c:123:1
msgid "@value: the value associated with the key."
msgstr ""

# used by: GHRFunc:
#: ghash.c:124:1
msgid "@user_data: user data passed to g_hash_table_remove()."
msgstr ""

# #-#-#-#-#  ghash.pot~ (PACKAGE VERSION)  #-#-#-#-#
# used by: g_hash_table_foreach_remove:
