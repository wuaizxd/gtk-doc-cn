# #used by: g_io_channel_set_encoding:
#: giochannel.c:1053:1
msgid "4. The current encoding is %NULL or UTF-8."
msgstr ""

# #used by: g_io_channel_set_encoding:
#: giochannel.c:1055:2
msgid "5. One of the (new API) read functions has just returned %G_IO_STATUS_EOF (or, in the case of g_io_channel_read_to_end (), %G_IO_STATUS_NORMAL)."
msgstr ""

