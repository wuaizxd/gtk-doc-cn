# used by: gksu_context_set_debug:
#: libgksu.c:1372:1
#: libgksu.c:1401:1
#: libgksu.c:1531:1
#: libgksu.c:1632:1
msgid "@value: TRUE or FALSE"
msgstr "@value: TRUE 或者 FALSE"

# #used by: gksu_context_set_login_shell:
#: libgksu.c:1374:2
msgid "Should the shell in which the command will be run be a login shell?"
msgstr "运行的命令的shell将成为登录的shell吗？"

# #used by: gksu_context_get_login_shell:
#: libgksu.c:1387:2
msgid "Finds out if the shell created by the underlying su process will be a login shell."
msgstr ""

# #used by: gksu_context_get_login_shell:
#: libgksu.c:1390:1
msgid "Returns: TRUE if the shell will be a login shell, FALSE otherwise."
msgstr "Returns: 如果shell成为登录的shell则返回TRUE,否则返回FALSE"

# #used by: gksu_context_set_keep_env:
#: libgksu.c:1403:3
msgid "Should the environment be kept as it is? Defaults to TRUE. Notice that setting this to FALSE may cause the X authorization stuff to fail."
msgstr "环境变量继续保持吗？默认为TRUE。 注意如果设置为FALSE那么X认证可能会失败。"

# #used by: gksu_context_get_keep_env:
#: libgksu.c:1417:2
msgid "Finds out if the environment in which the program will be run will be reset."
msgstr "确认是否将要运行的程序会重设环境."

# #used by: gksu_context_get_keep_env:
#: libgksu.c:1420:2
msgid "Returns: TRUE if the environment is going to be kept, FALSE otherwise."
msgstr "Returns: 如果环境信息继续保存则返回TRUE，否则返回FALSE"

# used by: gksu_context_set_description:
#: libgksu.c:1432:1
msgid "@description: a string to set the description for"
msgstr "@description: 包含描述信息的字符串"

# #used by: gksu_context_set_description:
#: libgksu.c:1434:3
msgid "Set the nice name for the action that is being run that the window that asks for the password will have.  This is only meant to be used if the default window is used, of course."
msgstr "给将要输入密码的窗口设置一个好点的名字。"

# used by: gksu_context_get_description:
#: libgksu.c:1448:1
msgid "@context: the #GksuContext you want to get the description from."
msgstr "@context: 你想要获取的描述信息来自#GksuContext"

# #used by: gksu_context_get_description:
#: libgksu.c:1450:3
msgid "Get the description that the window will have when the default function for requesting the password is called."
msgstr "如果要求输入密码的默认函数被调用，那么得到这个窗口的描述。"

# #used by: gksu_context_get_description:
#: libgksu.c:1454:1
msgid "Returns: a string with the description or NULL if not set."
msgstr "Returns: 包含描述信息的字符串，如果没有设置则返回NULL"

# used by: gksu_context_set_message:
#: libgksu.c:1465:1
msgid "@message: a string to set the message for"
msgstr "@message: 设置字符串消息"

# #used by: gksu_context_set_message:
#: libgksu.c:1467:2
msgid "Set the message that the window that asks for the password will have. This is only meant to be used if the default window is used, of course."
msgstr ""

# used by: gksu_context_get_message:
#: libgksu.c:1480:1
msgid "@context: the #GksuContext you want to get the message from."
msgstr "@context: 你想从获取的消息来自 #GksuContext"

# #used by: gksu_context_get_message:
#: libgksu.c:1482:3
msgid "Get the message that the window will have when the default function for requesting the password is called."
msgstr "如果默认的请求密码的函数被调用，得到这个窗口的消息。"

# #used by: gksu_context_get_message:
#: libgksu.c:1486:1
msgid "Returns: a string with the message or NULL if not set."
msgstr "Returns: 包含消息的字符串，如果没有设置就返回NULL"

# used by: gksu_context_set_alert:
#: libgksu.c:1497:1
msgid "@alert: a string to set the alert for"
msgstr "@alert: 为警告所设置的字符串"

# #used by: gksu_context_set_alert:
#: libgksu.c:1499:4
msgid "Set the alert that the window that asks for the password will have. This is only meant to be used if the default window is used, of course. This alert should be used to display messages such as 'incorrect password', for instance."
msgstr "设置输入密码窗口的警告信息. 如果默认窗口被使用。这个警告信息将被用来显示如\"无效的密码\"之类。"

# used by: gksu_context_get_alert:
#: libgksu.c:1514:1
msgid "@context: the #GksuContext you want to get the alert from."
msgstr "@context: 你想获取警告信息来自#GksuContext"

# #used by: gksu_context_get_alert:
#: libgksu.c:1516:3
msgid "Get the alert that the window will have when the default function for requesting the password is called."
msgstr ""

# #used by: gksu_context_get_alert:
#: libgksu.c:1520:1
msgid "Returns: a string with the alert or NULL if not set."
msgstr "Returns: 包含警告的字符串，如果没有设置则返回NULL"

# #used by: gksu_context_set_debug:
#: libgksu.c:1533:1
#: libgksu.c:1634:1
msgid "Set up if debuging information should be printed."
msgstr "设置debug，调试信息将被打印"

# used by: gksu_context_get_grab:
#: libgksu.c:1543:1
msgid "@context: the #GksuContext you want to ask whether a grab will be done."
msgstr ""

# #used by: gksu_context_get_grab:
#: libgksu.c:1545:2
msgid "Returns TRUE if gksu has been asked to do a grab on keyboard and mouse when asking for the password."
msgstr ""

# #used by: gksu_context_get_grab:
#: libgksu.c:1548:1
msgid "Returns: TRUE if yes, FALSE otherwise."
msgstr "Returns: 如果是则返回TRUE,否则返回FALSE"

# used by: gksu_context_set_launcher_context:
#: libgksu.c:1558:1
msgid "@context: the #GksuContext you want to set the sn context in"
msgstr "@context: 你要设置的sn上下文 #GksuContext"

# used by: gksu_context_set_launcher_context:
#: libgksu.c:1559:1
msgid "@sn_context: the #SnLauncherContext you want to set"
msgstr "@sn_context: 你想设置的 #SnLauncherContext"

# #used by: gksu_context_set_launcher_context:
#: libgksu.c:1561:6
msgid "Tell libgksu to use the given #SnLauncherContext for startup notification. Currently the library will use this to set DESKTOP_STARTUP_ID in the environment of the child and to issue initiate and complete events. Notice that you don't need to use this function unless you want to override gksu's default behavior on startup notification, since the library will create its own context."
msgstr ""

# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# #used by: gksu_context_set_launcher_context:
# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# #used by: gksu_context_get_launcher_context:
#: libgksu.c:1568:1
#: libgksu.c:1585:1
msgid "Returns: the #SnLauncherContext which is set, or NULL if none was set"
msgstr "Returns: 如果被设置，就返回 #SnLauncherContext, 否则，返回 NULL"

# used by: gksu_context_get_launcher_context:
#: libgksu.c:1581:1
msgid "@context: the #GksuContext you want to get the sn context from"
msgstr "@context: 你想得到的sn上下文 #GksuContext"

# #used by: gksu_context_get_launcher_context:
#: libgksu.c:1583:1
msgid "Gets the current startup notification launcher context"
msgstr "得到当前通知区域的上下文"

# used by: gksu_context_launch_initiate:
#: libgksu.c:1595:1
msgid "@context: the #GksuContext you want to initiate the launch for"
msgstr "@context: 你将要初始化的 #GksuContext"

# #used by: gksu_context_launch_initiate:
#: libgksu.c:1597:2
msgid "Initiates the launch, as far as Startup Notification is concerned; This will only be used internally, usually."
msgstr ""

# used by: gksu_context_launch_complete:
#: libgksu.c:1618:1
msgid "@context: the #GksuContext you want to complete the launch for"
msgstr "@context: 你想完全启动的 #GksuContext"

# #used by: gksu_context_launch_complete:
#: libgksu.c:1620:2
msgid "Completes the launch, as far as Startup Notification is concerned; This will only be used internally, usually."
msgstr ""

# #used by: gksu_context_get_debug:
#: libgksu.c:1646:2
msgid "Finds out if the library is configured to print debuging information."
msgstr "查明库是否配置成打印调试信息"

# #used by: gksu_context_get_debug:
#: libgksu.c:1649:1
msgid "Returns: TRUE if it is, FALSE otherwise."
msgstr "Returns: 如果是DEBUG 返回TRUE,否则返回FALSE"

# used by: gksu_context_free
#: libgksu.c:1659:1
msgid "@context: the #GksuContext to be freed."
msgstr "@context: 释放 #GksuContext"

# #used by: gksu_context_free
#: libgksu.c:1661:1
msgid "Frees the given #GksuContext."
msgstr "释放给定的 #GksuContext."

# #-#-#-#-#  libgksu.pot~ (libgksu 2.0.0)  #-#-#-#-#
# used by: gksu_su_full:
