# #used by: g_io_channel_set_encoding:
#: giochannel.c:1044:2
msgid "1. The channel was just created, and has not been written to or read from yet."
msgstr ""

# #used by: g_io_channel_set_encoding:
#: giochannel.c:1047:1
msgid "2. The channel is write-only."
msgstr ""

# #used by: g_io_channel_set_encoding:
#: giochannel.c:1049:3
msgid "3. The channel is a file, and the file pointer was just repositioned by a call to g_io_channel_seek_position(). (This flushes all the internal buffers.)"
