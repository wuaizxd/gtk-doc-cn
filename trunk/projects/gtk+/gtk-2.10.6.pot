msgid "@icon_size: the size of the icon that should be created."
msgstr ""

# #used by: gtk_action_create_icon:
#: gtkaction.c:870:2
msgid ""
"This function is intended for use by action implementations to create icons "
"displayed in the proxy widgets."
msgstr ""

# #used by: gtk_action_create_icon:
#: gtkaction.c:873:1
msgid "Returns: a widget that displays the icon for this action."
msgstr ""

# #used by: gtk_action_create_menu_item:
#: gtkaction.c:894:1
msgid "Creates a menu item widget that proxies for the given action."
msgstr ""

# #used by: gtk_action_create_menu_item:
#: gtkaction.c:896:1
msgid "Returns: a menu item connected to the action."
msgstr ""

# #used by: gtk_action_create_tool_item:
