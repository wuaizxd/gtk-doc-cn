#: gtkmenuitem.c:2556:1 gtkmenuitem.c:323:1 gtkmenuitem.c:338:1
msgid "Since: 2.16"
msgstr ""

#. used by: gtk_menu_item_get_label:
#: gtkmenuitem.c:2504:2
msgid ""
"Returns: The text in the @menu_item label. This is the internal string used "
"by the label, and must not be modified."
msgstr ""

#. used by: gtk_menu_item_set_use_underline:
#: gtkmenuitem.c:2520:1
msgid "@setting: %TRUE if underlines in the text indicate mnemonics"
msgstr ""

#. used by: gtk_menu_item_set_use_underline:
#: gtkmenuitem.c:2522:2
msgid ""
"If true, an underline in the text indicates the next character should be "
"used for the mnemonic accelerator key."
msgstr ""

#. used by: gtk_menu_item_get_use_underline:
#: gtkmenuitem.c:2550:2
msgid ""
"Checks if an underline in the text indicates the next character should be "
"used for the mnemonic accelerator key."
msgstr ""

#. used by: gtk_menu_item_get_use_underline:
#: gtkmenuitem.c:2553:2
msgid ""
"Return value: %TRUE if an embedded underline in the label indicates the "
"mnemonic accelerator key."
msgstr ""

#. used by: gtk_menu_item_set_reserve_indicator:
#: gtkmenuitem.c:2577:1
msgid "@reserve: the new value"
msgstr ""

#. used by: gtk_menu_item_set_reserve_indicator:
#: gtkmenuitem.c:2579:3
msgid ""
"Sets whether the @menu_item should reserve space for the submenu indicator, "
"regardless if it actually has a submenu or not."
msgstr ""

#. used by: gtk_menu_item_set_reserve_indicator:
#: gtkmenuitem.c:2583:2
msgid "There should be little need for applications to call this functions."
msgstr ""

#. #-#-#-#-#  gtkmenuitem.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_menu_item_set_reserve_indicator:
#. #-#-#-#-#  gtkmenuitem.pot (PACKAGE VERSION)  #-#-#-#-#
#. used by: gtk_menu_item_get_reserve_indicator:
#: gtkmenuitem.c:2586:1 gtkmenuitem.c:2616:1
msgid "Since: 3.0"
msgstr ""

#. used by: gtk_menu_item_get_reserve_indicator:
#: gtkmenuitem.c:2609:3
msgid ""
"Returns whether the @menu_item reserves space for the submenu indicator, "
"regardless if it has a submenu or not."
msgstr ""

#. used by: gtk_menu_item_get_reserve_indicator:
#: gtkmenuitem.c:2613:2
msgid ""
"Returns: %TRUE if @menu_item always reserves space for the submenu indicator"
msgstr ""

#. used by: GtkMenuItem:right-justified:
#: gtkmenuitem.c:273:2
msgid ""
"Sets whether the menu item appears justified at the right side of a menu bar."
msgstr ""

#. used by: GtkMenuItem:submenu:
#: gtkmenuitem.c:289:1
msgid "The submenu attached to the menu item, or %NULL if it has none."
msgstr ""

#. used by: GtkMenuItem:submenu:
#: gtkmenuitem.c:291:1
msgid "Since: 2.12"
msgstr ""

#. used by: GtkMenuItem:accel-path:
#: gtkmenuitem.c:304:3
msgid ""
"Sets the accelerator path of the menu item, through which runtime changes of "
"the menu item's accelerator caused by the user can be identified and saved "
"to persistant storage."
msgstr ""

#. used by: GtkMenuItem:label:
#: gtkmenuitem.c:321:1
msgid "The text for the child label."
msgstr ""

#. used by: GtkMenuItem:use-underline:
#: gtkmenuitem.c:336:1
msgid "%TRUE if underlines in the text indicate mnemonics."
msgstr ""

#. used by: GtkMenuItem:width-chars:
#: gtkmenuitem.c:399:1
msgid "The minimum desired width of the menu item in characters."
msgstr ""

#. used by: gtk_menu_item_new_with_mnemonic:
#: gtkmenuitem.c:463:2
msgid ""
"@label: The text of the button, with an underscore in front of the mnemonic "
"character"
msgstr ""

#. used by: gtk_menu_item_new_with_mnemonic:
#: gtkmenuitem.c:466:1
msgid "Creates a new #GtkMenuItem containing a label."
msgstr ""

#. used by: gtk_menu_item_new_with_mnemonic:
#: gtkmenuitem.c:468:2
msgid ""
"The label will be created using gtk_label_new_with_mnemonic(), so "
"underscores in @label indicate the mnemonic for the menu item."
msgstr ""

#. used by: gtk_menu_item_new_with_mnemonic:
#: gtkmenuitem.c:471:1
msgid "Returns: a new #GtkMenuItem"
msgstr ""
