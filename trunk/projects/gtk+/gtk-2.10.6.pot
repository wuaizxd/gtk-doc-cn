msgstr ""

# #used by: gtk_widget_get_visual:
#: gtkwidget.c:6394:1
msgid "Return value: the visual for @widget"
msgstr ""

# #used by: gtk_widget_get_settings:
#: gtkwidget.c:6408:2
msgid ""
"Gets the settings object holding the settings (global property settings, RC "
"file information, etc) used for this widget."
msgstr ""

# #used by: gtk_widget_get_settings:
#: gtkwidget.c:6411:3
msgid ""
